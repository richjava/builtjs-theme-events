{
  "theme": {
    "version": "1.0.0",
    "apiVersion": "1.2",
    "title": "Events",
    "description": "Theme for events websites with Tailwind styling",
    "imageUrl": "https://cdn.jsdelivr.net/gh/richjava/builtjs-theme-events/public/images/theme/theme.png",
    "repoName": "richjava/builtjs-theme-events",
    "gitProvider": "github",
    "repoUrl": "https://github.com/richjava/builtjs-theme-events",
    "demoUrl": "https://builtjs-theme-events.vercel.app",
    "modules": [
      {
        "name": "events"
      },
      {
        "name": "contact"
      },
      {
        "name": "getInformed"
      },
      {
        "name": "about"
      },
      {
        "name": "support"
      },
      {
        "name": "volunteer"
      },
      {
        "name": "sponsors"
      }
    ],
    "config": {
      "frontend": {
        "project": {
          "main": {
            "name": "nextjs",
            "config": {
              "router": "pages"
            }
          },
          "css": {
            "name": "tailwind"
          }
        },
        "node": {
          "dependencies": [
            "@sanity/block-content-to-react"
          ],
          "devDependencies": [
            "autoprefixer",
              "eslint",
              "eslint-config-next",
              "postcss",
              "tailwindcss",
              "tailwindcss-filters",
              "tailwindcss-spinner"
          ]
        }
      }
    }
  }
}
